{
	"editor.renderWhitespace": "all",
	"editor.fontFamily": "SourceHanCodeJP-Regular",
	"window.openFilesInNewWindow": "off",
	"editor.scrollBeyondLastLine": true,
	"editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?、。「」【】『』（）！？　てにをはがのともへでや",
	"editor.wordWrap": "on",
	"editor.formatOnPaste": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.snippetSuggestions": "top",
	"workbench.colorTheme": "Azurish",
	"editor.stablePeek": true,
	"[javascript]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": false,
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": false,
		"editor.formatOnSave": true
	},
	"[json]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": false,
		"editor.formatOnSave": true
	},
	"[vue]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": false,
		"editor.formatOnSave": true
	},
	"[go]": {
		"editor.insertSpaces": false,
		"editor.tabSize": 4,
		"editor.autoIndent": false,
		"editor.formatOnSave": true
	},
	"[perl]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4,
		"editor.autoIndent": false
	},
	"[python]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": false
	},
	"[lua]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4,
		"editor.autoIndent": false
	},
	"[php]": {
		"editor.insertSpaces": true,
		"editor.tabSize": 4,
		"editor.autoIndent": false,
	},
	"eslint.enable": true,
	"eslint.autoFixOnSave": true,
	"emmet.showSuggestionsAsSnippets": true,
	"prettier.tabWidth": 2,
	"prettier.useTabs": false,
	"eslint.validate": [
		"javascript",
		{
			"language": "typescript",
			"autoFix": true
		},
		{
			"language": "vue",
			"autoFix": true
		}
	],

	"php.executablePath": "/usr/local/opt/php@7.2/bin/php",
	"php.suggest.basic": false,
	"php.validate.enable": true,
	"php.validate.executablePath": "/usr/local/opt/php@7.2/bin/php",
	"php.validate.run": "onSave",
	"phpcs.standard": "PSR2",
	"phpcs.executablePath": "/Users/takeyu/.composer/vendor/bin/phpcs",
	"phpcs.autoConfigSearch": true,
	"symfony-vscode.detectCwd": false,
	"symfony-vscode.phpPath": "docker exec f5476dcd9e73 /bin/sh -c 'cd /var/www/project && /usr/local/bin/php \"$@\"' -- ",
	// "symfony-vscode.shellExecutable": "/bin/bash",
	// "symfony-vscode.shellCommand": "docker exec f5476dcd9e73 /bin/sh -c 'cd /var/www/project && /usr/local/bin/php \"$@\"' -- ",
	"emeraldwalk.runonsave": {
		"commands": [
			{
				"match": "\\.php$",
				"cmd": "prettier ${file} --write"
			}
		]
	},
	"window.zoomLevel": 0,
	"editor.fontSize": 11,
	"breadcrumbs.enabled": true,
	"bracketPairColorizer.consecutivePairColors": [
		"()",
		"[]",
		"{}",
		[
			"palegreen",
			"lightcyan",
			"LightSkyBlue"
		],
		"Red"
	],
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/vendor/**": true,
		"**/var/cache/**": true
	},
	"namespaceResolver.sortNatural": true,
	"namespaceResolver.sortAlphabetically": true,
	// "phpstan.enabled": true,
	// "phpstan.path": "/Users/takeyu/.composer/vendor/bin/phpstan",
	"phpstan.level": "5",
	"phpstan.autoloadFile": "/Users/takeyu/print-creator/app/autoload.php"
}
